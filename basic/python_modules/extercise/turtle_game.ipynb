    "import turtle\n",
    "window = turtle.Screen()\n",
    "\n",
    "geoff = turtle.Turtle()\n",
    "\n",
    "# program to draw a polygon on trtle \n",
    "def polygon(geoff, n,length):\n",
    "    for i in range(n):\n",
    "\n",
    "        angle = 360/n \n",
    "        geoff.fd(length)\n",
    "        geoff.lt(angle)\n",
    "polygon(geoff, 7, 70)\n",
    "\n",
    "window.exitonclick()"
   ]
  }
